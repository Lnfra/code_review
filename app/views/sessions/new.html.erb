<h1>Login</h1>

<div class="row">
  <div class="col-md-6">
    <%= form_for(:session, url: sessions_path) do |f| %>
        <%= f.label :email %>
        <%= f.text_field :email, class: 'form-control' %>

        <%= f.label :password %>
        <%= f.password_field :password, class: 'form-control' %>

        <br/>
        <%= f.submit "Login", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>




